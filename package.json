{
  "name": "@octamap/vite-hls",
  "version": "1.0.0",
  "description": "Making it super easy to use HLS video streaming instead of .mov, .mp4 and other slow video formats ",
  "type": "module",
  "keywords": [
    "vite",
    "vite-plugin",
    "compiler",
    "hls",
    "video-streaming",
    "stream video"
  ],
  "private": false,
  "scripts": {
    "build": "tsc",
    "publish-npmjs": "npm publish --access public"
